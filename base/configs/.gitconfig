[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[user]
	name = Branko Minic
	email = minicb79@gmail.com
	signingKey = ""
[push]
	default = simple
[color]
	ui = auto
[alias]
	me = !git rev-parse --abbrev-ref HEAD
	up = !git push origin -u $(git me)
	co = checkout
	cob = checkout -b
	b = branch
	ci = commit -m
	lp = log --pretty=oneline
    lg1 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(auto)%d%C(reset)' --all
	lg2 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(auto)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'
    lg = lg1
	lsl = branch --list
	lsa = branch --all
	lsd = branch --delete
	la = !git config -l | grep alias
	rh = reset --hard
	dev = !git lsl | grep dev | xargs git co
	main = checkout main
	master = checkout master
	s = status
    mdev = !git dev && git pull && git co - && git merge dev
    mmaster = !git master && git pull && git co - && git merge master
	mmain = !git main && git pull && git co - && git merge main
	mdevs = !git stash && git dev && git pull && git co - && git lsl | grep dev | xargs git merge && git stash pop
	mmains = !git stash && git main && git pull && git co - && git merge main && git stash pop
[gpg]
	program = gpg
[commit]
	gpgSign = false
[tag]
	forceSignAnnotated = false
[credential]
	helper = cache
[core]
	autocrlf = input
